{
  "name": "@ausdata/sdk",
  "version": "0.4.0",
  "description": "Official AusData SDK for forms, emails and business search",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "node test-script.mjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ausdata",
    "forms",
    "email",
    "business-search",
    "sdk"
  ],
  "author": "AusData Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ausdatascience/ausdata-sdk.git"
  },
  "devDependencies": {
    "eslint": "^9.13.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.48.0"
  }
}