{
  "name": "@ausdata/sdk",
  "version": "0.2.0",
  "description": "Official AusData SDK for forms and email templates",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "ausdata",
    "forms",
    "email",
    "sdk"
  ],
  "author": "AusData",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ausdatascience/ausdata-sdk"
  },
  "devDependencies": {
    "eslint": "^9.13.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0"
  }
}
