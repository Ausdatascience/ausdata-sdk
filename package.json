{
  "name": "@ausdata/sdk",
  "version": "0.4.0",
  "description": "Official AusData SDK for forms, emails and business search",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./styles": {
      "default": "./dist/styles.css"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "ausdata-sdk": "./scripts/cli.js"
  },
  "files": [
    "dist",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "./dist/styles.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "node test-script.mjs",
    "prepublishOnly": "npm run build",
    "generate-api-route": "node scripts/generate-api-route.js"
  },
  "keywords": [
    "ausdata",
    "forms",
    "email",
    "business-search",
    "sdk"
  ],
  "author": "AusData Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ausdatascience/ausdata-sdk.git"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.13.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.48.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
