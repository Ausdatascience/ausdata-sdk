{
  "themeToggle": {
    "background": {
      "light": "bg-gray-800",
      "dark": "dark:bg-gray-800"
    },
    "border": {
      "light": "border-gray-700",
      "dark": "dark:border-gray-700"
    },
    "iconLight": "text-gray-300",
    "iconDark": "text-gray-300"
  },
  "contactForm": {
    "background": {
      "light": "bg-black",
      "dark": "dark:bg-black"
    },
    "title": {
      "light": "text-white",
      "dark": "dark:text-white"
    },
    "subtitle": {
      "light": "text-gray-400",
      "dark": "dark:text-gray-400"
    },
    "label": {
      "light": "text-gray-300",
      "dark": "dark:text-gray-300"
    },
    "labelOptional": {
      "light": "text-gray-600",
      "dark": "text-gray-600"
    },
    "input": {
      "border": {
        "light": "border-gray-700",
        "dark": "dark:border-gray-700"
      },
      "background": {
        "light": "bg-gray-900",
        "dark": "dark:bg-gray-900"
      },
      "text": {
        "light": "text-white",
        "dark": "dark:text-white"
      },
      "focus": "focus:ring-gray-500"
    },
    "button": {
      "background": "bg-white",
      "hover": "hover:bg-gray-200",
      "disabled": "disabled:bg-gray-700",
      "text": "text-black"
    },
    "success": {
      "background": {
        "light": "bg-green-900/30",
        "dark": "dark:bg-green-900/30"
      },
      "border": {
        "light": "border-green-700",
        "dark": "dark:border-green-700"
      },
      "text": {
        "light": "text-green-300",
        "dark": "dark:text-green-300"
      }
    },
    "error": {
      "background": {
        "light": "bg-red-900/30",
        "dark": "dark:bg-red-900/30"
      },
      "border": {
        "light": "border-red-700",
        "dark": "dark:border-red-700"
      },
      "text": {
        "light": "text-red-300",
        "dark": "dark:text-red-300"
      }
    }
  }
}
