{
  "themeToggle": {
    "background": {
      "light": "bg-gray-100",
      "dark": "dark:bg-gray-100"
    },
    "border": {
      "light": "border-gray-300",
      "dark": "dark:border-gray-300"
    },
    "iconLight": "text-gray-700",
    "iconDark": "text-gray-700"
  },
  "contactForm": {
    "background": {
      "light": "bg-white",
      "dark": "dark:bg-white"
    },
    "title": {
      "light": "text-gray-900",
      "dark": "dark:text-gray-900"
    },
    "subtitle": {
      "light": "text-gray-600",
      "dark": "dark:text-gray-600"
    },
    "label": {
      "light": "text-gray-700",
      "dark": "dark:text-gray-700"
    },
    "labelOptional": {
      "light": "text-gray-400",
      "dark": "text-gray-400"
    },
    "input": {
      "border": {
        "light": "border-gray-300",
        "dark": "dark:border-gray-300"
      },
      "background": {
        "light": "bg-gray-50",
        "dark": "dark:bg-gray-50"
      },
      "text": {
        "light": "text-gray-900",
        "dark": "dark:text-gray-900"
      },
      "focus": "focus:ring-gray-500"
    },
    "button": {
      "background": "bg-gray-900",
      "hover": "hover:bg-gray-800",
      "disabled": "disabled:bg-gray-400",
      "text": "text-white"
    },
    "success": {
      "background": {
        "light": "bg-green-50",
        "dark": "dark:bg-green-50"
      },
      "border": {
        "light": "border-green-200",
        "dark": "dark:border-green-200"
      },
      "text": {
        "light": "text-green-800",
        "dark": "dark:text-green-800"
      }
    },
    "error": {
      "background": {
        "light": "bg-red-50",
        "dark": "dark:bg-red-50"
      },
      "border": {
        "light": "border-red-200",
        "dark": "dark:border-red-200"
      },
      "text": {
        "light": "text-red-800",
        "dark": "dark:text-red-800"
      }
    }
  }
}
